{
  "name": "aspect",
  "version": "0.0.1",
  "description": "Card-based identity exploration sandbox",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch server.ts",
    "start": "bun run server.ts",
    "build": "bun run scripts/build.ts",
    "build:analyze": "bun run scripts/analyze.ts",
    "check:size": "bun run scripts/check-size.ts",
    "check:types": "tsgo --noEmit",
    "lint": "oxlint",
    "bench": "bun test bench/bench.test.ts --timeout 60000",
    "test": "bun test test/",
    "test:e2e": "npx playwright test"
  },
  "dependencies": {
    "keybinds": "github:pterror/keybinds",
    "y-indexeddb": "^9.0.12",
    "y-websocket": "^3.0.0",
    "yjs": "^13.6.29"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.0",
    "@types/bun": "latest",
    "@typescript/native-preview": "^7.0.0-dev.20260124.1",
    "oxlint": "latest"
  }
}
